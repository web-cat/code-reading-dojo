<div class="jumbotron text-center">
  <p>Congradulations, you've completed the level!</p>
  <div class="form-horizontal form-group form-group-lg row">
    <div class="col-xs-10 col-xs-offset-1 col-sm-offset-4 col-sm-4 col-md-4">
      <button {{action "getCurrentUrl"}} class="btn btn-info btn-lg btn-block">Click here to see the errors</button>
        {{#if (eq clicked 'true')}}
          {{#link-to 'programs' currentUrl class="btn btn-primary btn-lg btn-block" invokeAction=(action 'unclicked')}}Click here for the next level{{/link-to}}
        {{/if}}
    </div>
  </div>
</div>
