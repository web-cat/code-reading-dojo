{{ember-notify messageStyle='bootstrap' classPrefix="custom-notify"}}


{{#if (eq clicked 'false')}}
<center>
  <h3>
  Press Start to Begin
</h3>
  <button {{action "clickCode" program.errorindexes}} class="btn btn-primary btn-lg" id="start-button">Start</button>
</center>
{{/if}}
{{#if (eq clicked 'true')}}
<center>
<button {{action "clickCode" program.errorindexes}} class="btn btn-primary btn-lg next-level-none" id="retry-button">Retry</button>
</center>
{{/if}}

{{#if (eq clicked 'true')}}
<span id="timer">
{{x-timer autoStart='true' format='HH:MM:SS' stopRequired=false}}
</span>
  <div id="scores">
    <span id="first-score" class="star-icon">☆</span>
    <span id="second-score" class="star-icon">☆</span>
    <span id="third-score" class="star-icon">☆</span>
    <span id="fourth-score" class="hide-star-icon">☆</span>
    <span id="fifth-score" class="hide-star-icon">☆</span>
  </div>
{{/if}}

<pre id="pre-code">
  <p id="p-code">
    {{program.code}}
  </p>
</pre>

